{
  "name": "camunda_admin_vue3.x",
  "version": "0.1.0",

  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "bpmn-js": "^8.7.0",
    "bpmn-js-properties-panel": "^0.43.1",
    "camunda-bpmn-moddle": "^5.1.2",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.2-beta.44",
    "sass-loader": "^10.2.0",
    "vue": "^3.0.0",
    "vue-axios": "^3.2.4",
    "vue-particles": "^1.0.9",
    "vue-router": "4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "^6.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "semi": [
        0
      ],
      "no-multiple-empty-lines": [
        0,
        {
          "max": 100
        }
      ],
      "no-mixed-spaces-and-tabs": [
        0
      ],
      "generator-star-spacing": 0,
      "arrow-parens": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
